  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby-starter-blog/issues"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.4.3",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-feed": "^2.0.7",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-postcss": "^2.0.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.37",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.3",
    "gatsby-remark-prismjs": "^3.0.1",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.33",
    "gatsby-transformer-remark": "^2.1.6",
    "gatsby-transformer-sharp": "^1.6.22",
    "lodash": "^4.17.5",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.0",
    "postcss-reporter": "^6.0.0",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-icons-kit": "^1.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "axe-core": "^3.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-gatsby": "^0.1.3",
    "babel-preset-react": "^6.24.1",
    "cypress": "^3.2.0",
    "cypress-axe": "^0.3.0",
    "eslint": "^5.6.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.6.1",
    "start-server-and-test": "^1.7.9"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicklemmon/nicklemmon-blog.git"
  },
  "scripts": {
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.jsx' 'src/**/*.md'",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "npm run test:jest && npm run test:e2e",
    "test:jest": "jest --config ./jest.config.js",
    "test:cypress": "npx cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:8000 test:cypress",
    "dev": "gatsby develop",
    "build": "gatsby build"
  }
}
